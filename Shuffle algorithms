#include <iostream>

using namespace std;

int main() {
	srand(time(nullptr));
	const char* barajas[] = {"A","B","C","D","E"};
	int n = 5;

	//Recorriendo desde el ultimo elemento
	for (int i = n - 1; i > 0; i--)
	{
		int j = rand() % (i + 1);
		swap(barajas[j], barajas[i]);
	}

	int x = 0;

	return 0;
}
